class test
   public static function basicTestInc(_start : int, _limit : int, _iterator: int) : int
      var i : int, summ : int, start : int, limit : int, iterator: int;

      start = _start;
      limit = _limit;
      iterator = _iterator;
      summ = 0;

      for i = start, limit, iterator  do
          summ = summ + 1;
      end

      return summ;
   end

