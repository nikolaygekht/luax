   public static function nestedLoopTest(str : string, strRep : int, rep : int) : string
      var i : int, j : int, substr : string, resStr : string;
      resStr = "";
      for i = 0, rep - 1 do
         substr = "";
         for j = 0, strRep - 1 do
            substr = substr .. str;
         end
         if i > 0 then
            resStr = resStr .. "_";
         end
         resStr = resStr .. substr;
      end
      return resStr;
   end

   public static function breakTest(arg : int) : int
      var i : int, summ : int;
      i = 5;
      summ = 0;
      for i = 5, 0, -1 do
        if arg == i then
           break;
        end
        summ = summ + i;
      end
      return summ;
   end

   public static function continueTest(arg : int) : int
      var i : int, summ : int;
      i = arg;
      summ = 0;
      for i = arg, 0, -1 do
        if i == 5 then
           continue;
        end
        summ = summ + i;
      end
      return summ;
   end
