class test
   static function test1(arg1 : int) : int
      var i : int;
      var j : int;

      j = 0;
      for i = 10, 0, -1 do
          if (i == 0) then
              continue;
          end

          j = i + 1;

          if (j == i) then
              break;
          end           
      end
      
      return j;
   end

   static function test2(arg1 : real) : real
    var i : real;
    var j : real;

    j = 0.0;
    for i = 10.0, 0.0, -1.0 do
        if (i == 0.0) then
            continue;
        end

        j = i + 1.0;

        if (j == i) then
            break;
        end           
    end
    
    return j;
 end
end